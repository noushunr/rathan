<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RathaanElectronics" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="description_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">#686565</item>
    </style>

    <style name="multilined_description_textview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center|left</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">#686565</item>
    </style>

    <style name="table_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">@color/black</item>

    </style>

    <style name="table_textview_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center|right</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textColor">@color/black</item>

    </style>

    <style name="table_textview_2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">@color/black</item>

    </style>

    <style name="table_textview_value_2">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center|right</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textColor">@color/black</item>

    </style>


    <style name="CustomCardViewStyle" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay_card_custom_corners</item>
    </style>


    <style name="ShapeAppearanceOverlay_card_custom_corners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">20dp</item>
        <item name="cornerSizeTopLeft">20dp</item>

    </style>

    <style name="Address_form_TextInputLayout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="boxStrokeWidth">.5dp</item>
        <item name="android:backgroundTint">@color/colorPrimary</item>
    </style>

    <style name="Address_form_TextInputEditText_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="MineCustomTabText">
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>


    <style name="nave_drawer_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:fontFamily">@font/roboto</item>

        <item name="android:gravity">center|left</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>


    </style>


    <style name="Theme.Atees_ui.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Atees_ui.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <!--<item name="android:windowCloseOnTouchOutside">false</item>-->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackground">     @android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>

</resources>